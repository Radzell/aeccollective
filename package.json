{
  "name": "acecollective",
  "version": "1.0.0",
  "author": "Jacob Bolda <me@jacobbolda.com>",
  "description": "The website for the AEC Collective, an Archiecture Engineering and Construction community.",
  "scripts": {
    "develop": "gatsby develop",
    "dev:hard": "yarn run clean && yarn run develop",
    "build": "yarn clean & yarn jest & gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf .cache && rm -rf public",
    "jest": "jest",
    "test": "yarn lint && yarn jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "pretty": "prettier --write \"{{src,plugins}/**/*.js,*.js}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbolda/aeccollective.git"
  },
  "license": "SEE LICENSE IN LICENESE FILE",
  "bugs": {
    "url": "https://github.com/jbolda/aeccollective/issues"
  },
  "homepage": "https://github.com/jbolda/aeccollective#readme",
  "dependencies": {
    "bulma": "0.6.2",
    "gatsby": "^2.0.78",
    "gatsby-image": "^2.0.25",
    "gatsby-link": "2.0.7",
    "gatsby-plugin-google-tagmanager": "^2.0.7",
    "gatsby-plugin-sass": "2.0.7",
    "gatsby-plugin-sharp": "^2.0.16",
    "gatsby-plugin-typography": "^2.2.4",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "2.0.12",
    "gatsby-theme-bulma-core": "^2.0.0-alpha.2",
    "gatsby-transformer-javascript-frontmatter": "2.0.5",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.18",
    "typography": "^0.16.18",
    "typography-theme-lawton": "^0.16.18"
  }
}
